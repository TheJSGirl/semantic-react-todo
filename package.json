{
  "name": "react-starter-v2",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Priyanka Negi",
  "license": "MIT",
  "scripts": {
    "quick": "npm run clean && npm install && npm start",
    "clean": "rm -rf .git && rm -rf _assets && git init && git add . && git commit -m \"initial commit\"",
    "clear": "rm -rf build",
    "cont": "node ./scripts/create_container",
    "comp": "node ./scripts/create_component",
    "build": "npm run clean && node ./scripts/copy_assets && webpack --config=config/webpack.prod.js",
    "start": "webpack-dev-server --config=config/webpack.dev.js",
    "test": "jest --watch",
    "test:cover": "jest --coverage"
  },
  "dependencies": {
    "babel-plugin-transform-class-properties": "^6.24.1",
    "fs-extra": "^5.0.0",
    "html-webpack-plugin": "^3.0.4",
    "identity-obj-proxy": "^3.0.0",
    "moment": "^2.21.0",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-datepicker": "^1.2.2",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "semantic-ui-css": "^2.3.0",
    "semantic-ui-react": "^0.78.3",
    "validator": "^9.4.1",
    "webpack": "3.5.4",
    "webpack-manifest-plugin": "^2.0.0-rc.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.1",
    "babel-loader": "^7.1.3",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.10",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.11",
    "jest-cli": "^22.4.2",
    "prettier-eslint": "^8.8.1",
    "react-addons-test-utils": "^15.6.2",
    "react-hot-loader": "3.0.0",
    "react-test-renderer": "^16.2.0",
    "style-loader": "^0.20.2",
    "webpack-dev-server": "^2.11.1"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "setupFiles": [
      "./scripts/Test/jestSetup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    }
  }
}
